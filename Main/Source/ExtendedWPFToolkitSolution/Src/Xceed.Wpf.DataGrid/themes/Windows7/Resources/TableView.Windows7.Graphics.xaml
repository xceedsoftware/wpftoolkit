      <!-- Needs Windows7 Resources -->
      <markup:DataGridThemeResourceDictionary Source="Windows7.Resources.xaml" />

   </ResourceDictionary.MergedDictionaries>

   <!-- Glyph that is displayed in the group headers to expand the group. -->
   <DataTemplate x:Key="expandGroupGlyph">
      <Grid>
         <Border Background="Transparent"
                 MinHeight="24"
                 MinWidth="24"
                 SnapsToDevicePixels="True">
            <Border CornerRadius="4"
                    BorderBrush="{StaticResource basicBlackBorderBrush}"
                    BorderThickness="0,0,1,1"
                    MinHeight="16"
                    MinWidth="16"
                    SnapsToDevicePixels="True"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center">
               <Border CornerRadius="3"
                       BorderBrush="{StaticResource basicWhiteBorderBrush}"
                       BorderThickness="0,0,1,1"
                       SnapsToDevicePixels="True">
               </Border>
            </Border>
         </Border>

         <Path  VerticalAlignment="Center"
                HorizontalAlignment="Center"
                Height="2"
                Width="10"
                Stretch="Fill"
                Opacity="1"
                Data="M 6.5,4.5 C6.5,4.5 2.5,4.5 2.5,4.5 "
                Stroke="#888"
                StrokeThickness="2"
                SnapsToDevicePixels="True" />
         <Path VerticalAlignment="Center"
               HorizontalAlignment="Center"
               Height="10"
               Width="2"
               Stretch="Fill"
               Opacity="1"
               Data="M 4,6 C4,6 4,2 4,2 "
               Stroke="#888"
               StrokeThickness="2"
               SnapsToDevicePixels="True" />
      </Grid>
   </DataTemplate>

   <!-- Glyph that is displayed in the group headers to collapse the group. -->
   <DataTemplate x:Key="collapseGroupGlyph">
      <Grid>
         <Border Background="Transparent"
                 MinHeight="24"
                 MinWidth="24"
                 SnapsToDevicePixels="True">
            <Border CornerRadius="4"
                    BorderBrush="{StaticResource basicBlackBorderBrush}"
                    BorderThickness="0,0,1,1"
                    MinHeight="16"
                    MinWidth="16"
                    SnapsToDevicePixels="True"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center">
               <Border CornerRadius="3"
                       BorderBrush="{StaticResource basicWhiteBorderBrush}"
                       BorderThickness="0,0,1,1"
                       SnapsToDevicePixels="True">
               </Border>
            </Border>
         </Border>

         <Path VerticalAlignment="Center"
               HorizontalAlignment="Center"
               Height="2"
               Width="10"
               Stretch="Fill"
               Opacity="1"
               Data="M 6.5,4.5 C6.5,4.5 2.5,4.5 2.5,4.5 "
               Stroke="#888"
               StrokeThickness="2"
               SnapsToDevicePixels="True" />
      </Grid>
   </DataTemplate>

